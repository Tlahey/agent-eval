{
  "name": "agent-eval-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "AgentEval â€“ AI coding agent evaluation framework",
  "scripts": {
    "build": "pnpm --filter agent-eval build",
    "build:ui": "pnpm --filter eval-ui build",
    "build:all": "pnpm build:ui && pnpm build && pnpm bundle-ui",
    "bundle-ui": "cp -r apps/eval-ui/dist packages/agent-eval/dist/ui",
    "dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build",
    "test": "pnpm --filter agent-eval test && pnpm --filter eval-ui test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "seed": "npx tsx scripts/seed-ledger.ts",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build:all && changeset publish",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "ai",
    "agent",
    "eval",
    "testing",
    "llm"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.30.1",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript-eslint": "^8.56.1"
  }
}
