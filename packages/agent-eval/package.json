{
  "name": "@dkt/agent-eval",
  "version": "0.1.0",
  "description": "AI coding agent evaluation framework with Vitest-like DX",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agenteval": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "agent",
    "eval",
    "testing",
    "llm",
    "judge"
  ],
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "ai": "^4.0.0",
    "chalk": "^5.3.0",
    "commander": "^13.0.0",
    "glob": "^11.0.0",
    "jiti": "^2.4.0",
    "ora": "^8.1.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
